@model NeoSoft_Tarun_Bommadevara.Web.ViewModels.EmployeeViewModel
    <div class="mb-2"><small><a asp-action="CreateEmployee">Add/Update Employee</a></small></div>
@{
    if (Model.Employees.Count() !=0)
    {
        <div class="overflow-auto">
            <table id="snv-main-table" class="table table-bordered table-responsive-sm">
                <thead>
                    <tr class="table-secondary">
                        <th scope="col">
                            Email
                        </th>
                        <th scope="col">
                            Country
                        </th>
                        <th scope="col">
                            State
                        </th>
                        <th scope="col">
                            City
                        </th>
                        <th scope="col">
                            Pan No
                        </th>
                        <th scope="col">
                            Gender
                        </th>
                        <th scope="col">
                            Isactive
                        </th>
                        <th scope="col">
                            Profile Image
                        </th>
                        <th scope="col">
                            Action
                        </th>
                    </tr>
                </thead>
                @{ @await Html.PartialAsync("Partial/_userTablePartial", Model) }
            </table>
        </div>
    }
    else
    {
        <div id="no-records-display" >
            No users available.
        </div>
    }
}

@section Scripts {
   
}